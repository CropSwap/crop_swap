


<h1>Login</h1>
  <main class="container login-form">

  <div class="form-group">

    <%=form_tag(session_create_path) do%>
     <%=label_tag :email %>
     <%=email_field_tag :email %>
  </div>

  <div class="form-group">

        <%=label_tag :password %>
        <%=password_field_tag :password %>
  </div>
        <%=submit_tag "Log In" %>
      <% end %>

  <%= link_to 'Sign Up', new_user_path %>

</main>
