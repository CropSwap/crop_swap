


  <h1>Login</h1>


  <%=form_tag(session_create_path) do%>
   <%=label_tag :email %> </label>
   <%=email_field_tag :email %>


      <p><%=label_tag :password %>
      <%=password_field_tag :password%></p>
  
      <%=submit_tag "Log In" %>
    <% end %>

  <%= link_to 'Sign Up', new_user_path %>

</form>
