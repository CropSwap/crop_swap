<table>
  <thead>
    <tr>
      <th>Swap type</th>
      <th>Crop</th>
      <th>Consumer</th>
      <th>Accepted</th>
      <th>Message</th>
      <th>Message response</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= trade.trade_type_id %></td>
      <td><%= trade.crop_id %></td>
      <td><%= trade.consumer_id %></td>
      <td><%= trade.accepted %></td>
      <td><%= trade.message %></td>
      <td><%= trade.message_response %></td>
      <td><%= link_to 'Show', trade %></td>
      <td><%= link_to 'Edit', edit_trade_path(trade) %></td>
      <td><%= link_to 'Destroy', trade, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  </tbody>
</table>
