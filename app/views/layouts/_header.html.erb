<div class="container">
<header class="navbar-fixed-top">
        <nav class="navbar-default navbar-fixed-top">
            <a href="#"class="img navbar-brand"><%= image_tag asset_url ("cropswap-logo.png") %></a>
            <div class="nav-list">
              <% if session[:user_id]%>
                <span class="nav-text"><%= link_to 'Logout', session_destroy_path, method: :delete, class: "nav-text"%></span>
              <% else %>
                <span> <%= link_to "Log in", session_new_path, class: "nav-text" %> </span>
              <% end %>
            </div>
        </nav>

</header>
</div>
